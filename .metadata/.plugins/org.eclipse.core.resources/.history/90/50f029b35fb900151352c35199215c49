import app.entity.Student;

import java.util.*;

@Component
public class Admin 
{
    public boolean checkUser(String username)
    {
        try
        {
            Student s = studentDao.findByNumberId(idNum);
            return s.checkIdNumber(idNum);
        }
        catch(Exception e)
        {
            return false;
        }
        
    }
   
}